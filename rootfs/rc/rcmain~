#if 1  //-----------------------
  "# rcmain: Plan 9 version \n"
  "if(~ $#home 0) home=/ \n"
  "if(~ $#ifs 0) ifs='\n' \n"
  "switch($#prompt){ \n"
  "case 0 \n"
  "prompt=('[lp49] ' '     ') \n"
  "case 1 \n"
  "prompt=($prompt '       ') \n"
  "} \n"
  "if(~ $rcname ?.out) prompt=('broken! ' '        ') \n"
  "     if(flag p) path=/t/bin \n"
  "     if not{ \n"
  "        finit \n"
  "        if(~ $#path 0) path=(. /t/bin /bin) \n"
  "     } \n"
  "fn sigexit \n"
  "if(! ~ $#cflag 0){ \n"
  "  if(flag l && /bin/test -r $home/lib/profile) . $home/lib/profile \n"
  "        status='' \n"
  "        eval $cflag \n"
  "  } \n"
  "  if not if(flag i){ \n"
  "   if(flag l && /bin/test -r $home/lib/profile) . $home/lib/profile \n"
  "         status='' \n"
  "         if(! ~ $#* 0) . $* \n"
  "         . -i '#d/0' \n"
  "  } \n"
  " if not if(~ $#* 0) . '#d/0' \n"
  " if not{ \n"
  "         status='' \n"
  "         . $* \n"
  " } \n"
  "#exit $status \n"
#else //------------------------
  "# rcmain: Plan 9 version \n"
  "if(~ $#home 0) home=/ \n"
  "if(~ $#ifs 0) ifs='\n' \n"
  "switch($#prompt){ \n"
  "case 0 \n"
  "prompt=('<lp49> ' '     ') \n"
  "case 1 \n"
  "prompt=($prompt '       ') \n"
  "} \n"


  "if(~ $#* 0) . '#d/0' \n"
  "if not{ \n"
  "        status='' \n"
  "        . $* \n"
  "} \n"
  "exit $status \n"
#endif //--------------------------
